{
  "name": "@jojovembh/cognito-cli-helper",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.1.5",
  "type": "module",
  "description": "A CLI to easily test AWS Cognito user functionalities.",
  "license": "MIT",
  "author": "Gustavo Andrade Ferreira",
  "homepage": "https://github.com/jojovem/cognito-cli-helper#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jojovem/cognito-cli-helper.git"
  },
  "bugs": {
    "url": "https://github.com/jojovem/cognito-cli-helper/issues"
  },
  "main": "dist/cli.js",
  "bin": {
    "cognito-cli": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "semantic-release": "semantic-release",
    "start": "node dist/cli.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.ts",
    "prepublishOnly": "npm run test && tsc --noEmit",
    "release": "npm run prepublishOnly && npm run clean && npm run build && semantic-release --no-ci",
    "commit": "cz",
    "prepare": "husky || true"
  },
  "types": "dist/cli.d.ts",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.848.0",
    "@aws-sdk/credential-providers": "^3.848.0",
    "cognito-srp-helper": "^2.3.4",
    "commander": "^14.0.0",
    "env-paths": "^3.0.0",
    "inquirer": "^12.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.16.5",
    "@types/prettier": "^2.7.3",
    "@types/yargs": "^17.0.33",
    "aws-sdk-client-mock": "^4.1.0",
    "commitizen": "^4.3.1",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "mock-fs": "^5.5.0",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.7",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "aws",
    "cli",
    "cognito"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
